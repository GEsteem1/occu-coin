<html>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>OccuCoin | Signup</title>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<link rel="stylesheet" href="../assets/css/main.css">
		<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" crossorigin="anonymous"></script>
	</head>
	<body class="is-preload">
		<!--[if lt IE 7]>
			<p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="#">upgrade your browser</a> to improve your experience.</p>
		<![endif]-->
		<% if (locals.error) { %>
			<h1>ERROR: <%= locals.error %></h1>
		<% } %>
		<div id="page-wrapper">
			<section id="main" class="container">
				<header>
					<h2></h2>
				</header>
				<div class="box">
					<form id="main" action="/signup" method="POST">
						<h2 for="use">Are you going to use this for...</h2>
						<input class="button primary" value="Personal Use" type="submit">
						<br>
						<br>
						<input class="button primary" value="Commercial Use" type="submit">
						<br>
						<br>
						<input class="button primary" value="Educational Use" type="submit">
					</form>
				</div>
			</section>
		</div>
	</body>
</html>